<main class="main">

    <!-- Page Title -->
    <div class="page-title accent-background">
        <div class="container d-lg-flex justify-content-between align-items-center">
            <div class="col-lg-4 col-md-6">
                <h2 class="mb-2 mb-lg-0 font-weight-bold">File my taxes</h2></br>
                <h1 class="mb-2 mb-lg-0">Wellcome <span>@ViewBag.UserFullName</span></h1>
            </div>
            <div class="col-lg-2 col-md-4">
                <nav class="breadcrumbs">
                    <ol>
                       @*  <li><a >Register</a></li>
                        <li class="current">File my taxes</li> *@
                    </ol>
                </nav>
            </div>
        </div>
    </div><!-- End Page Title -->
    <!-- File my taxes Section -->
    <section id="File-my-taxes" class="blog-posts section">

        <div class="container">
            <div class="row gy-4">

                <div class="col-lg-4 aos-init aos-animates" data-aos="fade-up" data-aos-delay="100">
                    <article class="position-relative h-100 rounded-md">

                        <div class="post-img position-relative overflow-hidden rounded-md-image">
                            <img src="~/assets/img/blog/blog-1.jpg" class="img-fluid rounded-md-image" alt="">
                            <span class="post-date d-none">December 12</span>
                        </div>

                        <div class="post-content d-flex flex-column">

                            <h3 class="post-title">
                                Assisted Filing
                            </h3>

                            <div class="meta d-flex align-items-center d-none">
                                <div class="d-flex align-items-center">
                                    <i class="bi bi-person"></i> <span class="ps-2">John Doe</span>
                                </div>
                                <span class="px-3 text-black-50">/</span>
                                <div class="d-flex align-items-center">
                                    <i class="bi bi-folder2"></i> <span class="ps-2">Politics</span>
                                </div>
                            </div>

                            <p>
                                Similique neque nam consequuntur ad non maxime aliquam quas. Quibusdam animi praesentium. Aliquam et laboriosam eius aut nostrum quidem aliquid dicta.
                            </p>

                            <hr>

                            <a asp-controller="Home" asp-action="TaxAssisted" class="readmore stretched-link"><span>Continue</span><i class="bi bi-arrow-right"></i></a>

                        </div>

                    </article>
                </div><!-- End File my taxes item -->

                <div class="col-lg-4 aos-init aos-animates" data-aos="fade-up" data-aos-delay="100">
                    <article class="position-relative h-100 rounded-md">

                        <div class="post-img position-relative overflow-hidden rounded-md-image">
                            <img src="~/assets/img/blog/blog-2.jpg" class="img-fluid rounded-md-image" alt="">
                            <span class="post-date d-none">March 19</span>
                        </div>

                        <div class="post-content d-flex flex-column">

                            <h3 class="post-title">Self Filing</h3>

                            <div class="meta d-flex align-items-center d-none">
                                <div class="d-flex align-items-center">
                                    <i class="bi bi-person"></i> <span class="ps-2">Julia Parker</span>
                                </div>
                                <span class="px-3 text-black-50">/</span>
                                <div class="d-flex align-items-center">
                                    <i class="bi bi-folder2"></i> <span class="ps-2">Economics</span>
                                </div>
                            </div>

                            <p>
                                Similique neque nam consequuntur ad non maxime aliquam quas. Quibusdam animi praesentium. Aliquam et laboriosam eius aut nostrum quidem aliquid dicta.s
                            </p>

                            <hr>

                            <a asp-controller="Home" asp-action="SelfFiling" class="readmore stretched-link"><span>Continue</span><i class="bi bi-arrow-right"></i></a>

                        </div>

                    </article>
                </div><!-- File my taxes item -->



            </div>
        </div>

    </section>
    <!-- /File my taxes -->

</main>