
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BaseAPIUrl": "https://api.taxfilinglk.com/",
  "cookieTimeoutMinutes": 30,

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "c://logs/TaxFiling-web-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
